{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting": "Information",
      "ModelContextProtocol": "Information",
      "MemoryServer": "Information"
    }
  },
  "MemoryServer": {
    "LLM": {
      "EnableGraphProcessing": true
    },
    "Embedding": {
      "EnableVectorStorage": true,
      "AutoGenerateEmbeddings": true,
      "UseHybridSearch": true,
      "TraditionalSearchWeight": 0.35,
      "VectorSearchWeight": 0.65
    }
  }
} 