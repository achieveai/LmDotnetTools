{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "AchieveAi.LmDotnetTools.AgUi": "Debug",
      "AchieveAi.LmDotnetTools.AgUi.Sample": "Debug"
    }
  },
  "AllowedHosts": "*",
  "AgUi": {
    "WebSocketPath": "/ag-ui/ws",
    "EnablePersistence": true,
    "DatabasePath": "agui-sample.db",
    "MaxSessionAgeHours": 24,
    "EnableDebugLogging": true
  }
}
