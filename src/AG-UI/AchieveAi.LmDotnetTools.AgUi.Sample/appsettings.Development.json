{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "AchieveAi.LmDotnetTools.AgUi": "Debug",
      "AchieveAi.LmDotnetTools.AgUi.Sample": "Debug",
      "AchieveAi.LmDotnetTools.AgUi.AspNetCore.WebSockets": "Debug",
      "AchieveAi.LmDotnetTools.AgUi.Protocol": "Debug"
    }
  },
  "AgUi": {
    "WebSocketPath": "/ag-ui/ws",
    "EnablePersistence": true,
    "DatabasePath": "agui-sample-dev.db",
    "MaxSessionAgeHours": 24,
    "EnableDebugLogging": true
  }
}
